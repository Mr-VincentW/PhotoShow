{
  "name": "photoshow",
  "version": "4.19.2",
  "description": "A browser extension for viewing high-definition images simply by hovering mouse on thumbnails.",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "extract-loader": "^5.1.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.5.0",
    "json-minimizer-webpack-plugin": "^3.1.0",
    "less": "^4.1.2",
    "less-loader": "^10.1.0",
    "mini-css-extract-plugin": "^2.4.2",
    "npm-run-all": "^4.1.5",
    "string-replace-loader": "^3.0.3",
    "strip-json-comments": "^3.1.1",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "zip-webpack-plugin": "^4.0.1"
  },
  "scripts": {
    "build": "webpack --env prod",
    "build-all": "run-p build build-edge build-firefox",
    "build-edge": "webpack --env prod --env vendor=edge",
    "build-firefox": "webpack --env prod --env vendor=firefox",
    "build-firefox-code-review": "webpack --env prod --env vendor=firefox-code-review",
    "dev": "webpack",
    "dev-edge": "webpack --env vendor=edge",
    "dev-firefox": "webpack --env vendor=firefox",
    "start": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mr-VincentW/PhotoShow.git#archive"
  },
  "keywords": [
    "image",
    "photo",
    "pic",
    "enlarge",
    "zoom",
    "productivity",
    "hover",
    "viewer",
    "browser",
    "extension",
    "addon",
    "chrome",
    "firefox",
    "edge"
  ],
  "author": "Vincent W.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mr-VincentW/PhotoShow/issues"
  },
  "homepage": "https://github.com/Mr-VincentW/PhotoShow#readme"
}
